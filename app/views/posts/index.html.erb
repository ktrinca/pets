<section id="categories">
  <% if params[:all] %>
    <div class="content"> 
      <div id="my-posts">
        <div class="container">
          <div class="row">
            <div class="col-lg-12 text-center">
              <h2>Listado</h2>
              <hr class="star-primary">
            </div>
          </div>
          <%= render @posts %>
          <div id="infinite-scrolling">
            <%= will_paginate %>
          </div>
        </div>
      </div>  
      
    </div>  
  <% else %>
    <div class="row">  
      <div class="content">
        <ul>
          <%= render('access_links') %> 
        </ul>  
      </div>
    </div>
  <% end %>
    
</section>    





  