<div id="body">
  <div id="content">
    
    <div class="search">
    </div> 
   
    <% if params[:all] %>
      <div class="content"> 
        <div id="my-posts">
          <%= render @posts %>
        </div>
        
        <div id="infinite-scrolling">
          <%= will_paginate %>
        </div>
      </div>  
    <% else %>
      <div class="row">  
        <div class="content">
          <ul>
            <%= render('access_links') %> 
          </ul>  
        </div>
      </div>
    <% end %>
    
    
  </div>

</div> 





  